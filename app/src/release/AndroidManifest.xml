<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eu.faircode.netguard">

    <permission
        android:name="gg.nexplay.booster.permission.ADMIN"
        android:description="@string/app_description"
        android:label="@string/app_name"
        android:protectionLevel="signature" />

    <uses-permission android:name="gg.nexplay.booster.permission.ADMIN" />

    <application>

        <receiver
            android:name=".WidgetAdmin"
            android:label="@string/app_name"
            android:permission="gg.nexplay.booster.permission.ADMIN">

            <intent-filter>
                <action android:name="gg.nexplay.booster.ON" />
                <action android:name="gg.nexplay.booster.OFF" />
                <action android:name="gg.nexplay.booster.LOCKDOWN_ON" />
                <action android:name="gg.nexplay.booster.LOCKDOWN_OFF" />
            </intent-filter>
        </receiver>
    </application>

</manifest>
